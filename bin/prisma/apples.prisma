datasource db {
  provider = "postgresql"
  url      = "DATABASE_URL"
}

generator client {
  provider        = "prisma-client-js"
}

model Apple {
  seeds   Int
  core    String  @unique

  @@map("applesf")
}

// models examples
model User {
  id        Int       @unique @default(autoincrement())
  createdAt DateTime  @default(now())
  email     String    @unique
  name      String?
  password  String
  whale     String
  cat       String
}
